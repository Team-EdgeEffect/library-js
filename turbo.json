{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    }
  }
}
